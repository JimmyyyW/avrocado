{
  "type": "record",
  "name": "AnalyticsEvent",
  "namespace": "com.example.avrocado",
  "doc": "A wide event schema with many fields for testing scroll behavior",
  "fields": [
    {"name": "event_id", "type": "string", "doc": "Unique event identifier"},
    {"name": "event_type", "type": "string", "doc": "Type of analytics event"},
    {"name": "event_version", "type": "string", "doc": "Schema version"},
    {"name": "timestamp", "type": "long", "doc": "Event timestamp in milliseconds"},
    {"name": "received_at", "type": "long", "doc": "Server receive timestamp"},
    {"name": "processed_at", "type": ["null", "long"], "default": null, "doc": "Processing completion timestamp"},
    {"name": "user_id", "type": ["null", "string"], "default": null, "doc": "Authenticated user ID"},
    {"name": "anonymous_id", "type": "string", "doc": "Anonymous tracking ID"},
    {"name": "session_id", "type": "string", "doc": "Session identifier"},
    {"name": "device_id", "type": ["null", "string"], "default": null, "doc": "Device identifier"},
    {"name": "ip_address", "type": ["null", "string"], "default": null, "doc": "Client IP address"},
    {"name": "user_agent", "type": ["null", "string"], "default": null, "doc": "Browser user agent string"},
    {"name": "referrer", "type": ["null", "string"], "default": null, "doc": "HTTP referrer"},
    {"name": "page_url", "type": ["null", "string"], "default": null, "doc": "Current page URL"},
    {"name": "page_title", "type": ["null", "string"], "default": null, "doc": "Current page title"},
    {"name": "page_path", "type": ["null", "string"], "default": null, "doc": "URL path component"},
    {"name": "page_search", "type": ["null", "string"], "default": null, "doc": "URL query string"},
    {"name": "page_hash", "type": ["null", "string"], "default": null, "doc": "URL hash fragment"},
    {"name": "viewport_width", "type": ["null", "int"], "default": null, "doc": "Browser viewport width"},
    {"name": "viewport_height", "type": ["null", "int"], "default": null, "doc": "Browser viewport height"},
    {"name": "screen_width", "type": ["null", "int"], "default": null, "doc": "Screen width"},
    {"name": "screen_height", "type": ["null", "int"], "default": null, "doc": "Screen height"},
    {"name": "screen_density", "type": ["null", "float"], "default": null, "doc": "Screen pixel density"},
    {"name": "timezone", "type": ["null", "string"], "default": null, "doc": "User timezone"},
    {"name": "locale", "type": ["null", "string"], "default": null, "doc": "User locale"},
    {"name": "language", "type": ["null", "string"], "default": null, "doc": "Browser language"},
    {"name": "platform", "type": ["null", "string"], "default": null, "doc": "Platform (web, ios, android)"},
    {"name": "os_name", "type": ["null", "string"], "default": null, "doc": "Operating system name"},
    {"name": "os_version", "type": ["null", "string"], "default": null, "doc": "Operating system version"},
    {"name": "browser_name", "type": ["null", "string"], "default": null, "doc": "Browser name"},
    {"name": "browser_version", "type": ["null", "string"], "default": null, "doc": "Browser version"},
    {"name": "device_type", "type": ["null", "string"], "default": null, "doc": "Device type (desktop, mobile, tablet)"},
    {"name": "device_manufacturer", "type": ["null", "string"], "default": null, "doc": "Device manufacturer"},
    {"name": "device_model", "type": ["null", "string"], "default": null, "doc": "Device model"},
    {"name": "app_name", "type": ["null", "string"], "default": null, "doc": "Application name"},
    {"name": "app_version", "type": ["null", "string"], "default": null, "doc": "Application version"},
    {"name": "app_build", "type": ["null", "string"], "default": null, "doc": "Application build number"},
    {"name": "library_name", "type": "string", "doc": "Analytics library name"},
    {"name": "library_version", "type": "string", "doc": "Analytics library version"},
    {"name": "campaign_source", "type": ["null", "string"], "default": null, "doc": "UTM source"},
    {"name": "campaign_medium", "type": ["null", "string"], "default": null, "doc": "UTM medium"},
    {"name": "campaign_name", "type": ["null", "string"], "default": null, "doc": "UTM campaign"},
    {"name": "campaign_term", "type": ["null", "string"], "default": null, "doc": "UTM term"},
    {"name": "campaign_content", "type": ["null", "string"], "default": null, "doc": "UTM content"},
    {"name": "geo_country", "type": ["null", "string"], "default": null, "doc": "Country code"},
    {"name": "geo_region", "type": ["null", "string"], "default": null, "doc": "Region/state code"},
    {"name": "geo_city", "type": ["null", "string"], "default": null, "doc": "City name"},
    {"name": "geo_postal_code", "type": ["null", "string"], "default": null, "doc": "Postal/ZIP code"},
    {"name": "geo_latitude", "type": ["null", "double"], "default": null, "doc": "Latitude"},
    {"name": "geo_longitude", "type": ["null", "double"], "default": null, "doc": "Longitude"},
    {"name": "is_bot", "type": "boolean", "default": false, "doc": "Whether request is from a bot"},
    {"name": "is_test", "type": "boolean", "default": false, "doc": "Whether this is test data"},
    {"name": "consent_analytics", "type": ["null", "boolean"], "default": null, "doc": "Analytics consent status"},
    {"name": "consent_marketing", "type": ["null", "boolean"], "default": null, "doc": "Marketing consent status"},
    {"name": "consent_personalization", "type": ["null", "boolean"], "default": null, "doc": "Personalization consent"},
    {"name": "ab_test_bucket", "type": ["null", "string"], "default": null, "doc": "A/B test bucket assignment"},
    {"name": "feature_flags", "type": {"type": "array", "items": "string"}, "default": [], "doc": "Active feature flags"},
    {"name": "custom_properties", "type": {"type": "map", "values": "string"}, "doc": "Custom event properties"},
    {"name": "metrics", "type": {"type": "map", "values": "double"}, "doc": "Numeric metrics"},
    {"name": "tags", "type": {"type": "array", "items": "string"}, "default": [], "doc": "Event tags"}
  ]
}
